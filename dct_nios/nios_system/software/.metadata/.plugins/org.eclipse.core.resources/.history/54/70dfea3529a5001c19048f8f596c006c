#include <stdio.h>

#include <stdlib.h>

#include <math.h>

#include <float.h>

#include <system.h>

#include <assert.h>

#include <io.h>



volatile int* dct_write_q = (int*) (AVALON_DCT_NIOS_0_BASE + 8);

volatile int* dct_write_size = (int*) (AVALON_DCT_NIOS_0_BASE);

volatile int* dct_write_data = (int*) (AVALON_DCT_NIOS_0_BASE + 4);



int main(void) {

	printf("Writing Q\n");
    *dct_write_q = 5;
    printf("Writing size\n");
    *dct_write_size = 3;

    printf("First Term: %d\n", IORD(AVALON_DCT_NIOS_0_BASE, 0 ));
    printf("Second Term: %d\n", *((volatile short*)(AVALON_DCT_NIOS_0_BASE + 4)));
    printf("Third Term Term: %d\n", *((volatile short*)(AVALON_DCT_NIOS_0_BASE + 8)));




    return 0;

}

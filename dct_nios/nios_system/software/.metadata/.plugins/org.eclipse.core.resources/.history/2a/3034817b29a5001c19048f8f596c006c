#include <stdio.h>

#include <stdlib.h>

#include <math.h>

#include <float.h>

#include <system.h>

#include <assert.h>

#include <io.h>







int main(void) {

	printf("Writing Q\n");
    IOWR_16DIRECT(AVALON_DCT_NIOS_0_BASE, 8, 5);
    printf("Writing size\n");
    IOWR_16DIRECT(AVALON_DCT_NIOS_0_BASE, 0, 3);

    printf("First Term: %d\n", IORD_16DIRECT(AVALON_DCT_NIOS_0_BASE, 0 ));
    printf("Second Term: %d\n", IORD_16DIRECT(AVALON_DCT_NIOS_0_BASE, 4 ));
    printf("Third Term Term: %d\n", IORD_16DIRECT(AVALON_DCT_NIOS_0_BASE, 8 ));




    return 0;

}
